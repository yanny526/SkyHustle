
[MASTER]
ignore=venv
jobs=1

[MESSAGES CONTROL]
disable=
    missing-docstring,
    invalid-name,
    too-few-public-methods,
    too-many-arguments,
    too-many-locals,
    line-too-long

[FORMAT]
max-line-length=120

[TYPECHECK]
ignored-modules=logging,os,base64,json,gspread,oauth2client,datetime,time,random
